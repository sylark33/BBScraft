
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout_2.cshtml";
}
<h2>YiCraftCore白名单</h2>
<div class="form-horizontal">
    <h4>10分钟之内填写邮箱的验证码</h4>
    <hr />
    <form method="post" action="/mail/Add_End">
        <div class="form-group">
            @Html.Label("请输入邮箱中的验证码", htmlAttributes: new { @class = "control-label col-md-2" })
            <div class="col-md-10">
                @Html.Editor("YZM", new { htmlAttributes = new { @class = "form-control" } })
            </div>
        </div>

        <input type="submit" value="提交审核" class="btn btn-default" />
    </form>
</div>



